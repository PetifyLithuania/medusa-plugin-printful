{
  "name": "medusa-plugin-printful",
  "version": "1.1.1",
  "description": "A Medusa plugin that enables Printful as a fulfillment provider and provides product and order synchronization.",
  "author": "Marco Freiberger <fsyntax@protonmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/fsyntax/medusa-plugin-printful.git"
  },
  "keywords": [
    "medusa",
    "medusa-plugin",
    "medusa-plugin-shipping",
    "medusa-plugin-fulfillment",
    "medusa-plugin-other"
  ],
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__ --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__ --extensions \".ts,.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@medusajs/medusa": "^1.7.15",
    "@types/lodash": "^4.14.191",
    "body-parser": "^1.20.2",
    "colorette": "^2.0.19",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "exponential-backoff": "^3.1.1",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "medusa-core-utils": "^1.1.39",
    "medusa-interfaces": "^1.3.6",
    "typeorm": "^0.2.36"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-typescript": "^7.14.5",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
